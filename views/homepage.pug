doctype html
html
  head
    title= `${title}`
    meta(name="viewport" content="width=device-width, initial-scale=1")
    link(rel="stylesheet", href="/css/style.css")
  body
  h1 Home
    table 
        thead
            tr
            th ID
            th Name
            th Engine
            th Brand
        tbody
            each obj in data
                tr
                    td #{obj.id}
                    td #{obj.properties.name}
                    td #{obj.properties.engine}
                    td #{obj.properties.brand}